{
    "name": "cmake-format",
    "displayName": "cmake-format",
    "description": "Format listfiles so they don't look like crap",
    "version": "0.0.1",
    "publisher": "cheshirekow",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:cmake"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "cmake",
                "aliases": [
                    "cmake",
                    "cmake-listfile"
                ],
                "extensions": [
                    "cmake"
                ]
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}